        <script>
    
            //facebook like and share
            (function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&appId=625319910894746&version=v2.0";
            fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
    
            $(document).ready(function () {

                // START Min BEDroom Slider
                $('#minBedSlider').slider({
                    formater: function (value) {
                        document.getElementById('minBedroomsDisplay').innerHTML = value;
                        return 'Current value: ' + value;
                    }
                });
                // END Min BEDroom Slider


                // START Min BEDroom Slider
                $('#minBedSlider').slider({
                    formater: function (value) {
                        document.getElementById('minBedroomsDisplay').innerHTML = value;
                        return 'Current value: ' + value;
                    }
                });
                // END Min BEDroom Slider


                // START Min BATHroom Slider
                $('#minBathSlider').slider({
                    formater: function (value) {
                        document.getElementById('minBathroomsDisplay').innerHTML = value;
                        return 'Current value: ' + value;
                    }
                });
                // END Min BATHroom Slider


                // START Bid Price Range Slider
                $("#bidPriceSlider").slider({
                    formater: function (value) {
                        return '$' + value;
                    }
                }).on('slide', function () {
                    var values = $(this).slider('getValue');
                    $('#MinBidPrice').val(values[0]);
                    $('#MaxBidPrice').val(values[1]);

                    document.getElementById('minBidDisplay').innerHTML = '$' + values[0];
                    document.getElementById('maxBidDisplay').innerHTML = '$' + values[1];
                })
                    .data('slider');
                // END Bid Price Range Slider

                // START Search Button inside Filter Pop Up
                $('#btn-real-estate-search').click(function () {
                    $('#form-real-estate-search').submit(function (e) {
                        var postData = new FormData(document.getElementById("form-real-estate-search"));
                    
                        var formURL = $(this).attr("action");

                        $.ajax(
                        {
                            url: formURL,
                            type: "POST",
                            data: postData,
                            success: function (data, textStatus, jqXHR) {
                                $("#real-estate-list").html(data);
                            },
                            error: function (jqXHR, textStatus, errorThrown) {
                                //if fails     
                            },
                            cache: false,
                            contentType: false,
                            processData: false
                            
                        });

                        e.preventDefault(); //STOP default action
                        e.unbind(); //unbind. to stop multiple form submit.
                        
                    });
                    $('#form-real-estate-search').submit();
                    
                });
           
            });

            // START type City
            function typeCity() {
                    var cityTyped = document.getElementById('search-City').value;
                    document.getElementById('cityDisplay').innerHTML = cityTyped;
            };
            // END type City

            // START click State Type Dropdown
            function clickState() {
                $(".form-control option").click(function () {
                    var dropDownValue = document.getElementById("stateDropDown");
                    var stateValue = dropDownValue.options[dropDownValue.selectedIndex].value;
                    var stateType = dropDownValue.options[dropDownValue.selectedIndex].text;
                    document.getElementById('stateDisplay').innerHTML = stateType;
                    //alert(thePropertyValue);
                    //alert(thePropertyType);
                });
            };
            // END click State Type Dropdown

            // START type Zip
            function typeZip() {
                var zipTyped = document.getElementById('search-Zip').value;
                alert("test");
                document.getElementById('zipDisplay').innerHTML = zipTyped;
            };
            // END type Zip

            // START click Property Type Dropdown
            function clickProperty() {
                $(".form-control option").click(function () {
                    var dropDownValue = document.getElementById("propertyDropDown");
                    var thePropertyValue = dropDownValue.options[dropDownValue.selectedIndex].value;
                    var thePropertyType = dropDownValue.options[dropDownValue.selectedIndex].text;
                    document.getElementById('propertyTypeDisplay').innerHTML = thePropertyType;
                    //alert(thePropertyValue);
                    //alert(thePropertyType);
                });
            };
            // END click Property Type Dropdown

        </script>

